{
    "name": "fukumori/laravel-blade-include-relative",
    "description": "Allows to include blade template with relative path based on current view.",
    "keywords": ["laravel", "blade", "relative"],
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Fukumori",
            "email": "levi@fukumori.com.br"
        }
    ],
    "require": {
        "illuminate/view": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "LFukumori\\BladeIncludeRelative\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "master": "5.8-dev"
        },
        "laravel": {
            "providers": [
                "LFukumori\\BladeIncludeRelative\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
